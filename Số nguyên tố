public class job3 {
	
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.print("Enter a sequence of positive integers: ");
		long n = sc.nextLong();
		long check = 0;
		
		while (n>0){
			check = n % 10;			
            if (n < 10) System.out.print(check);
			else System.out.print(check +", ");  
			n /=10;  
			}
	    }
	}
